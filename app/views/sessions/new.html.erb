<h1>Вход в систему</h1>
<%= form_with url: login_path do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <%= form.submit 'Войти' %>
<% end %>

<%= link_to 'Зарегистрироваться', signup_path %>
